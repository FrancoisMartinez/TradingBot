# Finnhub API Configuration
FINNHUB_API_KEY=your_finnhub_api_key_here
FINNHUB_WS_URL=wss://ws.finnhub.io

# Trading Service Configuration
TRADING_SERVICE_URL=http://localhost:3000
TRADING_SERVICE_API_KEY=your_trading_service_key

# Email Notification Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_SECURE=false
EMAIL_USER=your_email@gmail.com
EMAIL_PASSWORD=your_email_password
EMAIL_FROM=TradingBot <noreply@tradingbot.com>
EMAIL_TO=recipient@example.com

# News Scraper Configuration
NEWS_SCRAPER_INTERVAL_MS=300000
NEWS_SOURCES=https://example.com/news,https://another-source.com/feed

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
